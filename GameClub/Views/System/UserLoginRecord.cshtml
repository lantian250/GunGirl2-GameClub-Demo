@using GameClub.Models
@model PagedList<UserLoginRecord>
@{
    ViewBag.Title = "用户登录历史记录";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

@section main{
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12">
                <div>
                </div>
               <div class="card">
                    <div class="card-toolbar clearfix">
                        <h4>用户登录历史记录</h4>
                    </div>
                    <div class="card-body">
                        <div id="UserLoginRecordPageTable" class="table-responsive">
                            @Html.Partial("_UserLoginRecord", Model)
                        </div>
                    </div>
                </div> 
            </div>
        </div>
    </div>
}

@section script{
    @{ Html.RegisterMvcPagerScriptResource();}
}

